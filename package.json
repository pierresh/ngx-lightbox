{
  "name": "@jjmhalew/ngx-lightbox",
  "version": "18.0.0",
  "description": "A port >= Angular 18 for lightbox2",
  "main": "index.js",
  "dependencies": {
    "file-saver": "^2.0.5",
    "ngx-filesaver": "18.0.0"
  },
  "devDependencies": {
